{
  "stryker-config": {
    "ignore-methods": [
      "*Debug",
      "*Error",
      "*Warning"
    ],
    "mutate": [
      "!**/*Extensions.cs",
      "!**/MyDbContext.cs",
      "!**/Program.cs",
      "!**/Documentation/**",
      "!**/Proxy/**"
    ],
    "thresholds": {
      "break": 80
    },
    "additional-timeout": 15000
  }
}